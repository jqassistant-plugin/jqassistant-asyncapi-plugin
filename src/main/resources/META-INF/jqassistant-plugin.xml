<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v2.2"
                    id="jqa.plugin.openapi"
                    name="jQAssistant OpenApi Plugin"
                    version="${project.version}">
    <description>Provides a scanner for OpenAPI files.</description>
    <model>
        <class>org.jqassistant.plugin.asyncapi.api.model.ChannelDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.bindings.ChannelBindingsDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.ComponentsDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.ContactDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.ContractDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.CorrelationIDDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.ExternalDocsDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.InfoDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.LicenseDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.MessageDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.MessageExampleDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.MessageTraitDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.MultiFormatSchemaDescriptor</class>
