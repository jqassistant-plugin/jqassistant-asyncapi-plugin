        <class>org.jqassistant.plugin.asyncapi.api.model.OperationDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.OperationReplyAddressDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.OperationReplyDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.OperationTraitsDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.ParametersDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.ReferenceDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.SchemaDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.ServerDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.ServerVariableDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.SecuritySchemeDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.TagDescriptor</class>
