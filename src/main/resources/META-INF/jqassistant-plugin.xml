        <class>org.jqassistant.plugin.asyncapi.api.model.bindings.kafka.KafkaChannelBindingsDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.bindings.kafka.KafkaMessageBindingsDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.bindings.kafka.KafkaOperationBindingsDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.bindings.kafka.KafkaServerBindingsDescriptor</class>


        <class>org.jqassistant.plugin.asyncapi.api.model.bindings.ChannelBindingsDescriptor</class>
        <class>org.jqassistant.plugin.asyncapi.api.model.bindings.kafka.KafkaChannelBindingsDescriptor</class>

    </model>
    <scope>
        <class>org.jqassistant.plugin.asyncapi.api.AsyncApiScope</class>
    </scope>
    <scanner>
        <class>org.jqassistant.plugin.asyncapi.impl.AsyncAPIScannerPlugin</class>
    </scanner>
</jqassistant-plugin>